{
  "name": "maher/laravel-api-responder",
  "description": "Laravel package to standardize API responses",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Maher\\ApiResponder\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Maher\\ApiResponder\\Providers\\ApiResponderServiceProvider"
      ],
      "aliases": {
        "ApiResponse": "Maher\\ApiResponder\\Facades\\ApiResponse"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

